#include "hsp3cl.as"
#include "cmdexec.as"

mes "Content-type: text/html\n"
mes "<html><head><meta charset=\"utf-8\"></head><body>"

;現在のGPIO 17の値を取得 1 == ON, 0 == OFF
prev_led = gpioin(17)

if prev_led = 0{
	;ついていなかった
	mes "<p>オフでした。つけます</p>"
	next_led = 1
}else{
	; 
	mes "<p>オンでした。けします</p>"
	next_led = 0
}
gpio 17, next_led
mes "</body></html>"
end
