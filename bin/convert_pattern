#!/usr/bin/python
import sys

if len(sys.argv) != 2:
    print("Usage: python " + sys.argv[0] + "<filename>")
    sys.exit(1)

with open(sys.argv[1]) as f:
    signals = [s.strip() for s in f.readlines()]
    signals = signals[1:len(signals)]
    if len(signals) % 2 == 0:
        signals = signals[1:len(signals)]
    signals_spaces = map(lambda x: x.split(" ")[1], signals)

    cnt = 0
    result = ""
    for s in signals_spaces:
        cnt += len(s)
        if cnt >= 50:
            result += '\n'
            cnt = 0
        result += s + " "

    print(result)
