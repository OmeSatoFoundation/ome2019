#include "hsp3cl.as"
#include "hspcurl.as"
#include "htmlparser.as"

tmpfile = "/tmp/tv.html"
word = "ドラえもん"
curl "https://tv.yahoo.co.jp/search/?q=" + word, tmpfile
notesel buf
noteload tmpfile
htmltagattr buf, "p", "class:yjLS pb5p", buf
htmltext buf, "p", buf
htmltext buf, "a", buf

mes "Content-type: text/html\n"
mes "<html><head><meta charset=\"utf-8\"></head><body>"
mes buf
mes x
mes "</body></html>"
end

